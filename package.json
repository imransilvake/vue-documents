{
	"name": "vue-documents",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"preview": "vite preview",
		"test": "vitest",
		"test:ci": "CI=true vitest",
		"pretty:check": "prettier --check './**/*.{js,jsx,ts,tsx,json}'",
		"pretty:apply": "prettier --write './**/*.{js,jsx,ts,tsx,json}'",
		"lint:es": "npx eslint './**/*.{js,jsx,ts,tsx}' --quiet",
		"lint:es:fix": "npx eslint './**/*.{js,jsx,ts,tsx}' --fix",
		"scan:all": "yarn pretty:apply && yarn lint:es && yarn test:ci",
		"release": "release-it",
		"release:major": "yarn release-it -- major --ci",
		"release:minor": "yarn release-it -- minor --ci",
		"release:patch": "yarn release-it -- patch --ci"
	},
	"dependencies": {
		"vue": "^3.2.47"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.5.0",
		"@commitlint/config-conventional": "^17.4.4",
		"@release-it/conventional-changelog": "^5.1.1",
		"@types/node": "^18.15.5",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"@vitejs/plugin-vue": "^4.1.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-vue": "^9.10.0",
		"husky": "^8.0.0",
		"prettier": "^2.8.6",
		"release-it": "^15.9.3",
		"typescript": "^4.9.3",
		"vite": "^4.2.0",
		"vitest": "^0.29.7",
		"vue-tsc": "^1.2.0"
	}
}
